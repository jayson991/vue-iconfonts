  "description": "A Project For Vue To Use IconFont Intelligently",
  "main": "lib/vue-iconfonts.ssr.js",
  "unpkg": "lib/vue-iconfonts.min.js",
  "module": "lib/vue-iconfonts.esm.js",
  "scripts": {
    "build": "rollup -c",
    "build:es": "rollup -c --format es",
    "build:ssr": "rollup -c --format cjs",
    "build:unpkg": "rollup -c --format iife"
  },
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "vue",
    "icon",
    "font",
    "iconfont",
    "vue-iconfonts"
  ],
  "homepage": "https://github.com/jayson991/vue-iconfonts",
  "bugs": {
    "url": "https://github.com/jayson991/vue-iconfonts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:jayson991/vue-iconfonts.git"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "acorn": "^6.4.2",
    "minimist": "^1.2.0",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue-template-compiler": "^2.6.12"
  }
